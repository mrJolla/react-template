{
  "name": "react-template",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "prod": "yarn && yarn test:all:fix && yarn build && yarn preview",
    "ts-check": "tsc --noEmit",
    "knip": "knip",
    "test:all": "yarn ts-check && yarn test:eslint && yarn test:stylelint",
    "test:all:fix": "yarn ts-check && yarn test:eslint:fix && yarn test:prettier:fix && yarn test:stylelint:fix",
    "test:eslint": "eslint '**/*.{ts,tsx}'",
    "test:eslint:fix": "yarn test:eslint --fix",
    "test:stylelint": "stylelint '**/*.css' --aei",
    "test:stylelint:fix": "yarn test:stylelint --fix",
    "test:prettier:fix": "prettier --write '**/*.{ts,tsx,json,md}'",
    "test:commit": "commitlint -e",
    "biome:fix": "biome format --write ."
  },
  "dependencies": {
    "@neodx/svg": "0.6.1",
    "@tanstack/react-query": "5.14.2",
