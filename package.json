{
  "name": "react-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "prod": "yarn && yarn test:all:fix && yarn build && yarn preview",
    "ts-check": "yarn tsc --noEmit",
    "test:all": "yarn ts-check && yarn test:eslint && yarn test:stylelint",
    "test:all:fix": "yarn ts-check && yarn test:eslint:fix && yarn test:prettier && yarn test:stylelint:fix",
    "test:eslint": "eslint '**/*.{ts,tsx}'",
    "test:eslint:fix": "yarn test:eslint --fix",
    "test:stylelint": "stylelint '**/*.css' --aei",
    "test:stylelint:fix": "yarn test:stylelint --fix",
    "test:prettier": "prettier --write '**/*.{ts,tsx,json,md}'",
    "test:commit": "commitlint -e",
    "generate-sprite": "yarn sprite --group --root 'public/images/svg-sprite-assets' -o 'public/images/svg-sprites' -d '~/types/icon.ts'"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@neodx/svg": "0.5.0",
    "@tanstack/react-query": "4.35.3",
    "@tanstack/react-query-devtools": "4.35.3",
    "axios": "1.5.0",
    "classcat": "5.0.4",
    "copy-text-to-clipboard": "3.2.0",
    "dayjs": "1.11.10",
    "effector": "22.8.6",
    "effector-react": "22.5.4",
    "js-cookie": "3.0.5",
    "lightweight-charts": "4.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.46.2",
    "react-hot-toast": "2.4.1",
    "react-loading-skeleton": "3.3.1",
    "react-router-dom": "6.16.0",
    "rooks": "7.14.1"
  },
  "devDependencies": {
    "@commitlint/cli": "17.7.1",
    "@commitlint/config-conventional": "17.7.0",
    "@tanstack/eslint-plugin-query": "4.34.1",
    "@types/js-cookie": "3.0.4",
    "@types/node": "20.6.5",
    "@types/react": "18.2.22",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "6.7.2",
    "@typescript-eslint/parser": "6.7.2",
    "@vitejs/plugin-react-swc": "3.4.0",
    "autoprefixer": "10.4.16",
    "eslint": "8.50.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-plugin-dirs": "0.0.4",
    "eslint-plugin-effector": "0.11.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jest": "27.4.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.3",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-tailwindcss": "3.13.0",
    "eslint-plugin-typescript-sort-keys": "3.0.0",
    "eslint-plugin-unicorn": "48.0.1",
    "eslint-plugin-unused-imports": "3.0.0",
    "husky": "8.0.3",
    "lint-staged": "14.0.1",
    "postcss": "8.4.30",
    "prettier": "3.0.3",
    "prettier-plugin-tailwindcss": "0.5.4",
    "stylelint": "15.10.3",
    "stylelint-config-clean-order": "5.2.0",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-standard": "34.0.0",
    "stylelint-order": "6.0.3",
    "tailwindcss": "3.3.3",
    "typescript": "5.2.2",
    "vite": "4.4.9",
    "vite-plugin-svgr": "4.0.0",
    "vite-tsconfig-paths": "4.2.1"
  }
}
