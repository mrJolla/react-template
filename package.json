{
  "name": "react-template",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "prod": "yarn && yarn test:all:fix && yarn build && yarn preview",
    "ts-check": "tsc --noEmit",
    "test:all": "yarn ts-check && yarn test:eslint && yarn test:stylelint",
    "test:all:fix": "yarn ts-check && yarn test:eslint:fix && yarn test:prettier && yarn test:stylelint:fix",
    "test:eslint": "eslint '**/*.{ts,tsx}'",
    "test:eslint:fix": "yarn test:eslint --fix",
    "test:stylelint": "stylelint '**/*.css' --aei",
    "test:stylelint:fix": "yarn test:stylelint --fix",
    "test:prettier": "prettier --write '**/*.{ts,tsx,json,md}'",
    "test:commit": "commitlint -e",
    "generate-sprite": "yarn sprite --group --root 'assets/icons' -o 'public/imgs/svg-sprites' -d './src/shared/types/icon.ts'"
  },
  "dependencies": {
    "@neodx/svg": "0.5.0",
    "@tanstack/react-query": "4.36.1",
    "@tanstack/react-query-devtools": "4.36.1",
    "axios": "1.5.1",
    "classcat": "5.0.4",
    "dayjs": "1.11.10",
    "effector": "22.8.7",
    "effector-react": "22.5.4",
    "js-cookie": "3.0.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.47.0",
    "react-hot-toast": "2.4.1",
    "react-loading-skeleton": "3.3.1",
    "react-router-dom": "6.16.0",
    "rooks": "7.14.1"
  },
  "devDependencies": {
    "@commitlint/cli": "17.7.2",
    "@commitlint/config-conventional": "17.7.0",
    "@tanstack/eslint-plugin-query": "4.36.1",
    "@types/js-cookie": "3.0.4",
    "@types/react": "18.2.26",
    "@types/react-dom": "18.2.11",
    "@typescript-eslint/eslint-plugin": "6.7.4",
    "@typescript-eslint/parser": "6.7.4",
    "@vitejs/plugin-react-swc": "3.4.0",
    "autoprefixer": "10.4.16",
    "eslint": "8.51.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-dirs": "0.0.4",
    "eslint-plugin-effector": "0.11.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.4.3",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-tailwindcss": "3.13.0",
    "eslint-plugin-typescript-sort-keys": "3.0.0",
    "eslint-plugin-unicorn": "48.0.1",
    "eslint-plugin-unused-imports": "3.0.0",
    "husky": "8.0.3",
    "lint-staged": "14.0.1",
    "postcss": "8.4.31",
    "prettier": "3.0.3",
    "prettier-plugin-tailwindcss": "0.5.5",
    "stylelint": "15.10.3",
    "stylelint-config-clean-order": "5.2.0",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-standard": "34.0.0",
    "stylelint-order": "6.0.3",
    "tailwindcss": "3.3.3",
    "typescript": "5.2.2",
    "vite": "4.4.11",
    "vite-tsconfig-paths": "4.2.1"
  }
}
