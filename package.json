    "classcat": "5.0.4",
    "dayjs": "1.11.10",
    "effector": "23.1.0",
    "effector-react": "23.1.0",
    "js-cookie": "3.0.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.49.2",
    "react-hot-toast": "2.4.1",
    "react-loading-skeleton": "3.3.1",
    "react-router-dom": "6.21.0",
    "rooks": "7.14.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.4.1",
